body {
  overflow: hidden;
  font-family: 'bank-gothic', 'orbitron-medium', arial, helvetica, sans-serif;
  ;
  background: #151717;
  font-size: 13px;
  padding: 0px;
  margin: 0px;
}

.game_background {
  background-image: url(img/spacebg.png);
  background-image: url(img/spaceb2.jpg);
  background-size: cover;
  position: absolute;
  top: -50%;
  left: -50%;
  height: 200vh;
  width: 200vw;
  filter: blur(1px) grayscale(0.2);
  /*animation: spin 1440s linear infinite;*/
  z-index: -999;
}

.faction_name {
  color: #fff;
  text-shadow: 1px 2px #000;
  font-size: 2em;
  text-align: center;
  line-height: 2.2em;
}

.faction_sheets {
  display: grid;
  grid-auto-columns: 1fr;
  grid-auto-flow: column;
  grid-gap: 2em;
  padding: 2em;
}

.faction_sheet {
  height: calc(80vh - 6em);
  background-color: #0008;
  background-size: cover;
  background-position: center;
}

.faction_sheet_buttons {
  display: flex;
}

.faction_sheet_buttons div {
  color: #fff;
  margin: 1em 2em;
  cursor: pointer;
  font-size: 1.5em;
}

.faction_content {
  background-color: #0008;
  height: calc(100% - 5em);
  margin: 0 1em;
  color: #FFF;
  padding: 0 1em;
  overflow-y: auto;
}

.faction_sheet_unit_box {
  display: flex;
  margin-bottom: 2vh;
  flex-wrap: wrap;
}

.faction_sheet_token_box {
  margin: 1em auto;
  width: 40em;
  height: 10em;
  display: grid;
  grid-gap: 0 2em;
  grid-template-columns: auto auto auto;
  font-family: 'bank-gothic';
  transform: scale(0.6);
}
.faction_sheet_token_box_title {
  color: white;
  font-weight: bold;
  background-color: #555c;
  padding: 10px;
}
.faction_sheet_token_box .fa-dice-d20 {
  transform: translateX(-50%);
}
.faction_sheet_token_box .fa-futbol {
  transform: translateX(-50%);
}
.faction_sheet_token_box .fa-space-shuttle {
  transform: translateX(-50%);
}

.objectives_scorings {
  display: grid;
  position: absolute;
  bottom: 0px;
  left: 0px;
  width: 100%;
  grid-template-rows: auto;
}
.objectives_players_scored {
  height: 20px;
  z-index: 99;
}

.token_count {
  font-size: 1em;
  margin-top: 0px;
  display: block;
  color: #FFF;
  font-family: 'bank-gothic';
  text-shadow: 1px 1px #000;
}

.faction_sheet_active {
  overflow-y: auto;
  height: 100%;
  scroll-behavior: smooth;
}

.faction_sheet_token_box div {
  text-align: center;
  font-size: 1.5em;
}

.faction_sheet_token_box span {
  text-align: center;
}

.faction_sheet_action_card_box, .faction_sheet_planet_card_box, .faction_sheet_tech_box {
  clear: both;
  display: flex;
  flex-wrap: wrap;
  justify-content: flex-start;
  margin: 1em 1em 1em 1em;
  font-size:0.9em;
}

.faction_sheet_action_card_box, .faction_sheet_planet_card_box {
  margin: 1em 1em 17vw 1em;
}

